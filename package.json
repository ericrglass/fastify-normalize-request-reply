{
    "name": "fastify-normalize-request-reply",
    "version": "3.0.0",
    "description": "A Fastify plugin to normalize the request and reply to Express request and response",
    "main": "index.js",
    "scripts": {
        "test": "tap test/*.js --coverage --100 --coverage-report=text-lcov",
        "test:console": "tap test/*.js --coverage --100",
        "test:coverage": "tap test/*.js --cov --coverage-report=html",
        "test:coveralls": "tap test/*.js --cov --coverage-report=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ericrglass/fastify-normalize-request-reply.git"
    },
    "author": "Eric Glass <ericrglass@gmail.com>",
    "license": "MIT",
    "keywords": [
        "Fastify",
        "express",
        "web",
        "request",
        "response",
        "normalize"
    ],
    "devDependencies": {
        "cookie-parser": "^1.4.5",
        "coveralls": "^3.1.0",
        "fastify": "^3.2.0",
        "nunjucks": "^3.2.2",
        "point-of-view": "^3.8.0",
        "tap": "^12.6.1"
    },
    "dependencies": {
        "accepts": "~1.3.5",
        "content-disposition": "0.5.2",
        "content-type": "~1.0.4",
        "cookie": "0.3.1",
        "cookie-signature": "1.0.6",
        "encodeurl": "~1.0.2",
        "escape-html": "~1.0.3",
        "etag": "~1.8.1",
        "fastify-plugin": "^2.2.0",
        "fresh": "0.5.2",
        "on-finished": "~2.3.0",
        "parseurl": "~1.3.2",
        "proxy-addr": "^2.0.6",
        "qs": "6.5.2",
        "range-parser": "~1.2.0",
        "safe-buffer": "5.1.2",
        "send": "0.16.2",
        "statuses": "~1.4.0",
        "type-is": "~1.6.16",
        "utils-merge": "1.0.1",
        "vary": "~1.1.2"
    },
    "bugs": {
        "url": "https://github.com/ericrglass/fastify-normalize-request-reply/issues"
    },
    "homepage": "https://github.com/ericrglass/fastify-normalize-request-reply#readme"
}
